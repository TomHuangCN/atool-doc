{
  "name": "atool-doc",
  "version": "0.0.3",
  "description": "",
  "keywords": [
    "atool",
    "doc"
  ],
  "author": "jaredleechn <jaredleechn@gmail.com>",
  "homepage": "https://github.com/ant-tool/atool-doc#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ant-tool/atool-doc"
  },
  "bugs": {
    "url": "https://github.com/ant-tool/atool-doc/issues"
  },
  "license": "ISC",
  "main": "index.js",
  "files": [
    "lib",
    "bin",
    "tpl"
  ],
  "bin": {
    "atool-doc": "./bin/atool-doc.js"
  },
  "scripts": {
    "build": "rm -rf lib && ./node_modules/.bin/babel src --out-dir lib --ignore __tests__",
    "dev": "./node_modules/.bin/babel src --out-dir lib --watch --ignore __tests__",
    "lint": "eslint src",
    "eslint-fix": "eslint --fix src",
    "prepublish": "npm run build"
  },
  "peerDependencies": {
    "dora": "~0.3.0",
    "atool-build": "~0.6.0",
    "webpack": "~1.12.2"
  },
  "dependencies": {
    "atool-doc-js-loader": "~0.0.1",
    "atool-doc-md-loader": "~0.0.1",
    "atool-doc-util": "~0.0.1",
    "commander": "~2.9.0",
    "html-webpack-plugin": "~2.9.0",
    "koa-webpack-dev-middleware": "~1.1.0",
    "raw-loader": "~0.5.1"
  },
  "devDependencies": {
    "babel-cli": "~6.6.5",
    "babel-plugin-add-module-exports": "~0.1.2",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-stage-0": "~6.5.0",
    "eslint": "~2.3.0",
    "eslint-config-airbnb": "^6.0.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "pre-commit": [
    "lint"
  ],
}
